using System;

class Program
{
    static void Main()
    {
        int[,] arr = new int[5, 5];
        
        Random random = new Random();
        for (int i = 0; i < 5; i++)
        {
            for (int j = 0; j < 5; j++)
            {
                arr[i, j] = random.Next(-200, 201);
            }
        }
        
        Console.WriteLine("Massif:");
        for (int i = 0; i < 5; i++)
        {
            for (int j = 0; j < 5; j++)
            {
                Console.Write(arr[i, j] + "\t");  
            }
            Console.WriteLine(); 
        }
        
        int max = arr[0, 0];
        int min = arr[0, 0];
        int maxLines = 0, maxCol = 0;
        int minLines = 0, minCol = 0;
        
        for (int i = 0; i < 5; i++)
        {
            for (int j = 0; j < 5; j++)
            {
                if (arr[i, j] > max)
                {
                    max = arr[i, j];
                    maxLines = i;
                    maxCol = j;
                }

                if (arr[i, j] < min)
                {
                    min = arr[i, j];
                    minLines = i;
                    minCol = j;
                }
            }
        }
       
        Console.WriteLine("\nMaximum element: " + max);
        Console.WriteLine("Coordinates of the maximum element: [" + maxLines + ", " + maxCol + "]");

        Console.WriteLine("\nMinimal element: " + min);
        Console.WriteLine("\nCoordinates of the minimal element: [" + minLines + ", " + minCol + "]");
    }
}
