using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("This program generates a multiplication table for that number from 1 to 10.");
        Console.WriteLine("Enter a number");
        int num = int.Parse(Console.ReadLine());
        
        Console.WriteLine($"Multiplication table for a number {num}:");
        
        for (int a = 1; a <= 10; a++)
        {
            int result = num * a; 
            Console.WriteLine($"{num} x {a} = {result}"); 
        }
    }
}
