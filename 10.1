using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter first and last name:");
        string text = Console.ReadLine(); 
        
        string[] words = text.Split(' ');
        
        if (words.Length >= 2)
        {
            char firstLetterLastname = char.ToLower(words[1][0]);
            char firstLetterName = char.ToLower(words[0][0]);
            
            if (firstLetterName == firstLetterLastname)
            {
                Console.WriteLine("The last name begins with the same letter as the first name.");
            }
            else
            {
                Console.WriteLine("Last name does not start with the same letter as the first name.");
            }
        }
    }
}
